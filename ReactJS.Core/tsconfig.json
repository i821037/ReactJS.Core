{
  "compilerOptions": {
    "module": "commonjs",
    "baseUrl": "Scripts",
    "noImplicitAny": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [ "es6", "dom" ],
    "types": [
      "react",
      "react-dom"
    ],
    "jsx": "preserve",
    //"outDir": "wwwroot/js",
    "sourceMap": true,
    "target": "es5"
    //"moduleResolution": "node"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "compileOnSave": true
}